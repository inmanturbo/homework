{
    "name": "inmanturbo/homework",
    "description": "Laravel Passport extension for WorkOS UserManagement authentication",
    "type": "library",
    "keywords": [
        "laravel",
        "passport",
        "workos",
        "oauth",
        "authentication"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0",
        "laravel/passport": "^12.0",
        "laravel/workos": "^0.1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Inmanturbo\\Homework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Inmanturbo\\Homework\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "pest",
        "test-coverage": "pest --coverage",
        "lint": "pint",
        "lint-test": "pint --test"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Inmanturbo\\Homework\\HomeworkServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}